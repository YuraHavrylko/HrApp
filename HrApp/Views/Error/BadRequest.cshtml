@model dynamic

    @{
        ViewBag.Title = "Bad Request";
    }


    <div class="content-wrapper" style="min-height: 1228px;">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                400 Error Page
            </h1>
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="error-page">
                <h2 class="headline text-yellow"> 400</h2>

                <div class="error-content">
                    <h3><i class="fa fa-warning text-yellow"></i> Oops! Request which you send is not valid.</h3>

                    <p>
                        Please try again later.
                        Meanwhile, you may <a href="/">return to account</a>
                    </p>


                </div>
                <!-- /.error-content -->
            </div>
            <!-- /.error-page -->
        </section>
        <!-- /.content -->
    </div>
