@model dynamic

    @{
        ViewBag.Title = "Forbidden";
    }

    <div class="content-wrapper" style="min-height: 1228px;">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                403 Error Page
            </h1>
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="error-page">
                <h2 class="headline text-yellow"> 403</h2>

                <div class="error-content">
                    <h3><i class="fa fa-warning text-yellow"></i> Oops! Access forbidden, You don't have anought permission.</h3>

                    <p>
                        We could not get access, ask for permission by admin.
                        Meanwhile, you may <a href="/">return to account</a>
                    </p>


                </div>
                <!-- /.error-content -->
            </div>
            <!-- /.error-page -->
        </section>
        <!-- /.content -->
    </div>
